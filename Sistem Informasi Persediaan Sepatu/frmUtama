Private Sub Timer1_Timer()
Me.Caption = Right$(Me.Caption, Len(Me.Caption) - 1) + Left$(Me.Caption, 1)
StatusBar1.Panels(1) = Format(Date, "dd mmmm yyyy")
StatusBar1.Panels(2) = Format(Time, "hh:mm:ss")
End Sub

 
  
