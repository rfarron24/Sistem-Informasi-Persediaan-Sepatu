Private Sub a1_Click()
With Crpt1
                         .DataFiles(0) = App.Path & "\Database\db.Mdb"
                         .ReportFileName = "D:\R125_PROGRAM\Laporan\biodata.rpt"
                         .RetrieveDataFiles
                         .WindowState = crptMaximized
                         .Action = 2
 End With
End Sub

 

 

